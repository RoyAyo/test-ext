{
    "manifest_version": 3,
    "name": "Lightforth linkedin Collector",
    "version": "1.0",
    "permissions": ["cookies", "tabs", "storage"],
    "host_permissions": ["https://*.linkedin.com/"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
    {
      "matches": ["https://*.linkedin.com/*"],
      "js": ["content.js"]
    }
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }
  